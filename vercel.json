{
  "functions": {
    "src/app/api/webhooks/stripe/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/webhooks/stripe-fixed/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        }
      ]
    }
  ]
}