{
  "functions": {
    "src/app/api/webhooks/stripe/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,PUT,DELETE,OPTIONS"
        }
      ]
    }
  ],
  "deploymentProtection": {
    "bypassPaths": [
      "/api/webhooks/stripe",
      "/api/test-webhook",
      "/api/create-manual-report",
      "/api/test-supabase"
    ]
  }
}